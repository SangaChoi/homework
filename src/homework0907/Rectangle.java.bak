public class Rectangle{



///////////Field
	int width;
	int length;
	String color;
	int area;
	int perimeter;


///////////////Constructor
	public Rectangle(){
	}

/////////////method
	public int area(){
		area=width*length;
		return area;
	}
	public int perimeter(){
		perimeter=width*2+length*2;
		return perimeter;
	}


///////////////setter method
	public void setColor(String col){
		color=col;	
	}
	public void setWidth(int wid){
		width=wid;
	}

	public void setLength(int leng){
		length=leng;
	}

	
////////////getter method
	public String getColor(){
		return color;
	}
	public int getWidth(){
		return width;
	}
	public int getLength(){
		return length;
	}

}